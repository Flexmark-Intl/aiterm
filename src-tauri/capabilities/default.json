{
  "$schema": "https://schemas.tauri.app/capability.schema.json",
  "identifier": "default",
  "description": "Default capability for aiTerm",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "shell:allow-open",
    "log:default"
  ]
}
